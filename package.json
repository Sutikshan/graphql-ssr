{
  "name": "graphql-ssr",
  "version": "1.0.0",
  "description":
    "Sample Implementation GraphQL, Server Side Rendering in React+NodeJS",
  "main": "index.js",
  "scripts": {
    "start": "webpack -w & nodemon server.js",
    "update-test": "jest --updateSnapshot",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sutikshan/graphql-ssr.git"
  },
  "keywords": ["graphql", "ssr", "nodejs", "react", "redux"],
  "author": "Anand Dubey",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Sutikshan/graphql-ssr/issues"
  },
  "homepage": "https://github.com/Sutikshan/graphql-ssr#readme",
  "eslintConfig": {
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "rules": {
      "no-console": 0
    },
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "es6": true,
      "node": true,
      "browser": true
    },
    "globals": {
      "it": true,
      "expect": true
    }
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "react"
    ],
    "plugins": ["transform-object-rest-spread"]
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./setupTests.js"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.1.12",
    "apollo-client": "^2.2.8",
    "apollo-client-preset": "^1.0.8",
    "apollo-link-http": "^1.5.4",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "express-graphql": "^0.6.12",
    "graphql-tag": "^2.9.1",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-apollo": "^2.1.3",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.7.0",
    "graphql": "^0.13.2",
    "jest": "^22.4.3",
    "nodemon": "^1.12.5",
    "react-test-renderer": "^16.3.2",
    "watchman": "^1.0.0",
    "webpack": "^3.10.0",
    "webpack-node-externals": "^1.6.0"
  }
}
