{
  "name": "graphql-ssr",
  "version": "1.0.0",
  "description": "Sample Implementation GraphQL, Server Side Rendering in React+NodeJS",
  "main": "index.js",
  "scripts": {
    "start": "webpack -w & nodemon server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sutikshan/graphql-ssr.git"
  },
  "keywords": [
    "graphql",
    "ssr",
    "relay-modern",
    "nodejs",
    "react",
    "redux"
  ],
  "author": "Anand Dubey",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Sutikshan/graphql-ssr/issues"
  },
  "homepage": "https://github.com/Sutikshan/graphql-ssr#readme",
  "eslintConfig": {
    "extends": [
      "eslint:recommended"
    ],
    "rules": {
      "no-console": 0
    },
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "es6": true,
      "node": true
    }
  },
  "babel": {
    "presets": [
      "env",
      "react"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "dependencies": {
    "express": "^4.16.3",
    "express-graphql": "^0.6.12",
    "graphql": "^0.13.2",
    "cors": "^2.8.4",
    "isomorphic-fetch": "^2.2.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "serialize-javascript": "^1.4.0"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "nodemon": "^1.12.5",
    "webpack": "^3.10.0",
    "webpack-node-externals": "^1.6.0"
  }
}
